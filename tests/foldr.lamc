
let foldr : forall a. forall b. (a -> b -> b) -> b -> list a -> b =
  \ @a -> \ @b ->
  \ (f : a -> b -> b) -> \ (z : b) ->
  letrec go : list a -> b =
    \ (xs : list a) -> case uncons xs return b of {
      nil -> z;
      cons (y : a) (ys : list a) -> f y (go ys)
    };
  in
  go
in
let max2 : int -> int -> int =
  \ (elem : int) -> \ (acc : int) -> if elem > acc return int then elem else acc
in
